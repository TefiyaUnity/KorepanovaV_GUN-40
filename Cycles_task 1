// Задание 1
namespace HomeWork
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int a = 0, b = 1;

            Console.WriteLine("Первые 10 чисел Фибоначчи:");


            for (int i = 0; i < 10; i++)
            {
                Console.Write(a + " ");
                int next = a + b;
                a = b;
                b = next;
            }
        }
    }
}
