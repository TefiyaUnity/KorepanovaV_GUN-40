// Задание 3
namespace HomeWork
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Таблица умножения от 1 до 5:");

            // Внешний цикл: перебираем множители от 1 до 5 (строки таблицы)
            for (int i = 1; i <= 5; i++)
            {
                // Внутренний цикл: перебираем второй множитель от 1 до 5 (столбцы таблицы)
                for (int j = 1; j <= 5; j++)
                {
                    // Выводим произведение i * j
                    Console.Write($"{i * j}\t"); // \t — табуляция для выравнивания столбцов
                }
                Console.WriteLine();
            }
        }
    }
}
